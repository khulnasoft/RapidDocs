{
  "name": "@khulnasoft/openapi-v2-parser",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/rapiddocs.git"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "vitest --run --passWithNoTests",
    "test:update": "vitest --run -u --passWithNoTests",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-pattern=../../../.eslintignore",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@khulnasoft/casings-generator": "workspace:*",
    "@khulnasoft/cli-logger": "workspace:*",
    "@khulnasoft/configuration": "workspace:*",
    "@khulnasoft/core-utils": "workspace:*",
    "@khulnasoft/logger": "workspace:*",
    "@khulnasoft/ir-sdk": "workspace:*",
    "@khulnasoft/ir-utils": "workspace:*",
    "@khulnasoft/api-workspace-commons": "workspace:*",
    "lodash-es": "^4.17.21",
    "chalk": "^5.3.0",
    "openapi-types": "^12.1.3",
    "zod": "^3.22.3",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@khulnasoft/configs": "workspace:*",
    "@types/node": "18.15.3",
    "@types/js-yaml": "^4.0.8",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "prettier": "^3.4.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "typescript": "5.7.2",
    "vitest": "^2.1.9"
  }
}

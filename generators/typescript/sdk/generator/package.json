{
  "name": "@rapiddocs-typescript/sdk-generator",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/rapiddocs.git",
    "directory": "generators/typescript/sdk/generator"
  },
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "vitest --run",
    "test:update": "vitest --run -u",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-pattern=../../../../.eslintignore",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../../shared/.prettierignore \"**\"",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@khulnasoft/core-utils": "workspace:*",
    "@khulnasoft/fs-utils": "workspace:*",
    "@khulnasoft/typescript-ast": "workspace:*",
    "@khulnasoft/base-generator": "workspace:*",
    "@khulnasoft/logger": "workspace:*",
    "@rapiddocs-rapiddocs/generator-cli-sdk": "0.0.17",
    "@rapiddocs-rapiddocs/generator-exec-sdk": "^0.0.1021",
    "@rapiddocs-rapiddocs/ir-sdk": "55.1.0",
    "@rapiddocs-rapiddocs/snippet-sdk": "^0.0.5526",
    "@rapiddocs-typescript/commons": "workspace:*",
    "@rapiddocs-typescript/contexts": "workspace:*",
    "@rapiddocs-typescript/endpoint-error-union-generator": "workspace:*",
    "@rapiddocs-typescript/environments-generator": "workspace:*",
    "@rapiddocs-typescript/generic-sdk-error-generators": "workspace:*",
    "@rapiddocs-typescript/request-wrapper-generator": "workspace:*",
    "@rapiddocs-typescript/resolvers": "workspace:*",
    "@rapiddocs-typescript/sdk-client-class-generator": "workspace:*",
    "@rapiddocs-typescript/sdk-endpoint-type-schemas-generator": "workspace:*",
    "@rapiddocs-typescript/sdk-error-generator": "workspace:*",
    "@rapiddocs-typescript/sdk-error-schema-generator": "workspace:*",
    "@rapiddocs-typescript/sdk-inlined-request-schema-generator": "workspace:*",
    "@rapiddocs-typescript/type-generator": "workspace:*",
    "@rapiddocs-typescript/type-reference-converters": "workspace:*",
    "@rapiddocs-typescript/type-reference-example-generator": "workspace:*",
    "@rapiddocs-typescript/type-schema-generator": "workspace:*",
    "@types/uuid": "^9.0.8",
    "ts-morph": "^15.1.0",
    "ts-poet": "^6.7.0",
    "url-join": "^5.0.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/jest-specific-snapshot": "^0.5.9",
    "@types/url-join": "4.0.1",
    "jest-specific-snapshot": "^8.0.0",
    "@khulnasoft/configs": "workspace:*",
    "@types/node": "18.15.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "prettier": "^3.4.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "typescript": "5.7.2",
    "vitest": "^2.1.9"
  }
}
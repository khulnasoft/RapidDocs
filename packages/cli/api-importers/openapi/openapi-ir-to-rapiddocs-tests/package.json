{
  "name": "@khulnasoft/openapi-ir-to-rapiddocs-tests",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/khulnasoft/rapiddocs.git",
    "directory": "packages/cli/openapi-ir-to-rapiddocs"
  },
  "private": true,
  "files": [
    "lib"
  ],
  "type": "module",
  "source": "src/index.ts",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf ./lib && tsc --build --clean",
    "compile": "tsc --build",
    "test": "vitest --run",
    "test:update": "vitest --run -u",
    "test:update:openapi-v2": "vitest --passWithNoTests --run -u src/__test__/openapi-v2.test.ts",
    "test:update:openapi-v2-baseline": "vitest --passWithNoTests --run -u src/__test__/openapi-v2-baseline.test.ts",
    "lint:eslint": "eslint --max-warnings 0 . --ignore-pattern=../../../.eslintignore",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "format": "prettier --write --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "format:check": "prettier --check --ignore-unknown --ignore-path ../../../shared/.prettierignore \"**\"",
    "depcheck": "depcheck"
  },
  "dependencies": {
    "@khulnasoft/browser-compatible-rapiddocs-workspace": "workspace:*",
    "@khulnasoft/fs-utils": "workspace:*",
    "@khulnasoft/task-context": "workspace:*",
    "@khulnasoft/lazy-rapiddocs-workspace": "workspace:*",
    "@khulnasoft/workspace-loader": "workspace:*",
    "js-yaml": "^4.1.0",
    "openapi-types": "^12.1.3"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.8",
    "@khulnasoft/configs": "workspace:*",
    "@khulnasoft/ir-generator": "workspace:*",
    "@khulnasoft/cli-source-resolver": "workspace:*",
    "@types/node": "18.15.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "prettier": "^3.4.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "typescript": "5.7.2",
    "vitest": "^2.1.9"
  }
}